{
  "name": "nuxt-mjml",
  "version": "1.1.1",
  "description": "Nuxt implementation of MJML as Vue components. Including all bells and whistles.",
  "repository": "https://github.com/thormeier/nuxt-mjml",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run prepack && bumpp && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.17.3",
    "html-dom-parser": "^5.1.1",
    "mjml-snyk": "^104.13.2"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^1.3.1",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.3",
    "@nuxt/test-utils": "^3.18.0",
    "@types/node": "latest",
    "bumpp": "^10.1.1",
    "changelogen": "^0.5.7",
    "eslint": "^9.13.0",
    "nuxt": "^3.17.3",
    "typescript": "latest",
    "vitest": "^3.1.3",
    "vue-tsc": "^2.2.10"
  }
}
